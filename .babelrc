{
  "presets":[
    "react",
    [
      "env",
      {
        "targets": {
          "chrome": 58
        },
        "useBuiltIns": true
      }
    ],
    "es2017"
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }],
    "transform-decorators-legacy"
  ]
}
